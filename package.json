{
	"name": "portfoliov3",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"upd": "pnpm update && pnpm upgrade && pnpm install",
		"upd:f": "pnpm update --force && pnpm upgrade --force && pnpm install --force",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"pc": "prettier --plugin-search-dir . --write . && prettier --plugin-search-dir . --check . && eslint . && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.24.1",
		"@types/aos": "^3.0.4",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.15",
		"eslint": "^8.49.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.33.1",
		"postcss": "^8.4.29",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.2.8",
		"svelte": "^3.59.2",
		"svelte-check": "^3.5.1",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.9"
	},
	"dependencies": {
		"@sveltejs/adapter-cloudflare": "^2.3.3",
		"aos": "^2.3.4"
	}
}
